#include <iostream>
using namespace std;

//Membuat function tanpa parameter dan tanpa nilai kembalian
void function ()
{
		cout << "Nama: Muhammad Yusfi Khatami" <<endl;
		cout << "NIM: ETE10190110" <<endl;
		cout << "Prodi: Teknik Elektromedis" <<endl;
		cout << "Mata Kuliah: Bahasa Pemrograman" <<endl;
		cout << endl;
		cout << "Tugas tentang Function ini tanpa parameter dan tanpa nilai kembalian" <<endl;
}

int main ()
{
		function ();
		return 0;
}
